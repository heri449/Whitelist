local HttpService = game:GetService("HttpService")
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- URL de tu whitelist
local whitelistURL = "https://raw.githubusercontent.com/heri449/Whitelist/main/White%20list"

-- Descargar la whitelist
local success, response = pcall(function()
    return game:HttpGet(whitelistURL)
end)

if success then
    -- Separar por saltos de línea
    local whitelistData = response:split("\n")
    local isWhitelisted = false

    -- Revisar si el nombre del jugador está en la lista
    for _, user in ipairs(whitelistData) do
        if user:lower():gsub("%s+", "") == LocalPlayer.Name:lower() then
            isWhitelisted = true
            break
        end
    end

    if isWhitelisted then
        -- Ejecutar el script principal
        loadstring(game:HttpGet("https://raw.githubusercontent.com/heri449/KINGscript09/main/KING"))()
    else
        -- Usuario no autorizado
        LocalPlayer:Kick("No estás en la whitelist.")
    end
else
    warn("Error al obtener la whitelist:", response)
    LocalPlayer:Kick("Error al verificar la whitelist.")
end
